<pgrm> -> {<stmt>}
<stmt> -> <cmd>(<args>)
		| <var> = <expr>
		| <var> = (<expr>)
		| if (<bool_expr>) then
				{<stmt>}
			[else
				{<stmt>}]
<args> -> <expr>
		-> <type>
<type> -> mat | scl
<expr> -> <expr> (* | + | - | /) <expr>
		| <cmd>(<expr>)
		| <var>
<var> -> <scl> | <matrix>
<scl> -> \b[a-z][A-Za-z]*\b
<mtx> -> \b[A-Z][A-Za-z]*\b
<cmd> -> \b(new | del | prn | inp | rref | ref | inv)